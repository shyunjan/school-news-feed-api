{
  "name": "school-news-feed-api",
  "version": "0.0.1",
  "description": "School News Feed API server",
  "author": "ShinHyunJae",
  "private": false,
  "license": "UNLICENSED",
  "module": "esnext",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "build:swc": "swc --ignore \"**/*.spec.ts\" --delete-dir-on-start --out-dir dist/src/ src/",
    "build:dev": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "build:prod": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev-backup": "nest start --watch",
    "migrate": "npx prisma migrate dev",
    "local": "dotenv -- npm run start:local",
    "start:local": "rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js",
    "start-github-action:prod": "rimraf dist && nest build",
    "test:e2e": "dotenv -- npm run test:e2e:local",
    "test:e2e:local": "jest --detectOpenHandles --config test/jest.e2e.js",
    "eslint": "eslint \"./src/**/*.ts\" && tsc --noemit",
    "compile": "tsc",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/static": "^6.9.0",
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.1.2",
    "@nestjs/cqrs": "^10.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-fastify": "^9.4.3",
    "@nestjs/swagger": "^6.1.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "fastify": "^4.13.0",
    "mongoose": "^7.6.10",
    "nest-winston": "^1.7.0",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "swagger-ui-express": "^4.5.0",
    "typescript": "^4.9.5",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.68",
    "@types/bcrypt": "^5.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "29.0.3",
    "@types/node": "^14.11.2",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "gts": "^5.3.0",
    "jest": "^29.0.3",
    "lint-staged": "^13.2.3",
    "npm": "^9.7.2",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.0.2",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,css,scss,md}": [
      "npx prettier --write",
      "echo \"npm run lint --prefix webapp --fix\"",
      "eslint"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
